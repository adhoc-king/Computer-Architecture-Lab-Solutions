    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55dd6b5f51d0_0, 0, 1;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x55dd6b5f4f40_0, 0, 32;
T_1.0 ;
    %load/vec4 v0x55dd6b5f4f40_0;
    %cmpi/s 16, 0, 32;
    %jmp/0xz T_1.1, 5;
    %load/vec4 v0x55dd6b5f52c0_0;
    %load/vec4 v0x55dd6b5f4f40_0;
    %part/s 1;
    %store/vec4 v0x55dd6b5f5000_0, 0, 1;
    %delay 2, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55dd6b5f4e80_0, 0, 1;
