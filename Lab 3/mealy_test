S_0x55dd6b5d46c0 .scope module, "mealy_test" "mealy_test" 2 1;
 .timescale 0 0;
v0x55dd6b5f4e80_0 .var "clk", 0 0;
v0x55dd6b5f4f40_0 .var/i "i", 31 0;
v0x55dd6b5f5000_0 .var "inp", 0 0;
v0x55dd6b5f5100_0 .net "outp", 0 0, v0x55dd6b5f4b40_0;  1 drivers
v0x55dd6b5f51d0_0 .var "rst", 0 0;
v0x55dd6b5f52c0_0 .var "sequence", 15 0;
S_0x55dd6b5d4840 .scope module, "m1" "mealy" 2 7, 3 1 0, S_0x55dd6b5d46c0;
