v0x55dd6b5d4a80_0 .net "clk", 0 0, v0x55dd6b5f4e80_0;  1 drivers
v0x55dd6b5f4a80_0 .net "inp", 0 0, v0x55dd6b5f5000_0;  1 drivers
v0x55dd6b5f4b40_0 .var "outp", 0 0;
v0x55dd6b5f4c10_0 .net "rst", 0 0, v0x55dd6b5f51d0_0;  1 drivers
v0x55dd6b5f4cd0_0 .var "state", 1 0;
E_0x55dd6b5ccb60 .event posedge, v0x55dd6b5f4c10_0, v0x55dd6b5d4a80_0;
    .scope S_0x55dd6b5d4840;
T_0 ;
    %wait E_0x55dd6b5ccb60;
    %load/vec4 v0x55dd6b5f4c10_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 2;
    %assign/vec4 v0x55dd6b5f4cd0_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x55dd6b5f4b40_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x55dd6b5f4cd0_0;
    %dup/vec4;
    %pushi/vec4 0, 0, 2;
    %cmp/u;
    %jmp/1 T_0.2, 6;
    %dup/vec4;
    %pushi/vec4 1, 0, 2;
    %cmp/u;
    %jmp/1 T_0.3, 6;
    %dup/vec4;
    %pushi/vec4 2, 0, 2;
    %cmp/u;
    %jmp/1 T_0.4, 6;
    %pushi/vec4 0, 0, 2;
    %assign/vec4 v0x55dd6b5f4cd0_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x55dd6b5f4b40_0, 0;
    %jmp T_0.6;
T_0.2 ;
    %load/vec4 v0x55dd6b5f4a80_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.7, 8;
    %pushi/vec4 2, 0, 2;
    %assign/vec4 v0x55dd6b5f4cd0_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x55dd6b5f4b40_0, 0;
    %jmp T_0.8;
T_0.7 ;
    %pushi/vec4 1, 0, 2;
    %assign/vec4 v0x55dd6b5f4cd0_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x55dd6b5f4b40_0, 0;
T_0.8 ;
    %jmp T_0.6;
T_0.3 ;
    %load/vec4 v0x55dd6b5f4a80_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.9, 8;
    %pushi/vec4 2, 0, 2;
    %assign/vec4 v0x55dd6b5f4cd0_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x55dd6b5f4b40_0, 0;
    %jmp T_0.10;
T_0.9 ;
    %pushi/vec4 0, 0, 2;
    %assign/vec4 v0x55dd6b5f4cd0_0, 0;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x55dd6b5f4b40_0, 0;
T_0.10 ;
    %jmp T_0.6;
T_0.4 ;
    %load/vec4 v0x55dd6b5f4a80_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.11, 8;
    %pushi/vec4 0, 0, 2;
    %assign/vec4 v0x55dd6b5f4cd0_0, 0;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x55dd6b5f4b40_0, 0;
    %jmp T_0.12;
T_0.11 ;
    %pushi/vec4 1, 0, 2;
    %assign/vec4 v0x55dd6b5f4cd0_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x55dd6b5f4b40_0, 0;
T_0.12 ;
    %jmp T_0.6;
T_0.6 ;
    %pop/vec4 1;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x55dd6b5d46c0;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55dd6b5f4e80_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55dd6b5f51d0_0, 0, 1;
    %pushi/vec4 22386, 0, 16;
    %store/vec4 v0x55dd6b5f52c0_0, 0, 16;
